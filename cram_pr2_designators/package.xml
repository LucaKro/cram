<package>
  <name>cram_pr2_designators</name>
  <version>0.2.0</version>
  <description>
    Resolving manipulation designators for PR2 robot
  </description>
  <author>Lorenz Moesenlechner</author>
  <maintainer email="musterman@fictional.de">Add Maintainer here</maintainer>
  <license>BSD</license>
  <url>http://ros.org/wiki/cram_pr2_designators</url>
  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>cram_plan_occasions_events</build_depend>
  <build_depend>cram_robot_interfaces</build_depend>
  <build_depend>cram_prolog</build_depend>
  <build_depend>cram_designators</build_depend>
  <build_depend>cram_semantic_map_utils</build_depend>
  <build_depend>cram_designators</build_depend>
  <build_depend>cram_semantic_map</build_depend>
  <build_depend>cram_transforms_stamped</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>cram_plan_occasions_events</run_depend>
  <run_depend>cram_robot_interfaces</run_depend>
  <run_depend>cram_prolog</run_depend>
  <run_depend>cram_designators</run_depend>
  <run_depend>cram_semantic_map_utils</run_depend>
  <run_depend>cram_designators</run_depend>
  <run_depend>cram_semantic_map</run_depend>
  <run_depend>cram_transforms_stamped</run_depend>

</package>
